{
  "name": "@whatwg-node/server",
  "version": "0.7.2",
  "description": "Fetch API compliant HTTP Server adapter",
  "repository": {
    "type": "git",
    "url": "ardatan/whatwg-node",
    "directory": "packages/server"
  },
  "author": "Arda TANRIKULU <ardatanrikulu@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@whatwg-node/fetch": "^0.8.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "18.14.6",
    "fastify": "4.14.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
