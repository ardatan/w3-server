{
  "name": "@whatwg-node/oas-client",
  "version": "0.0.0",
  "description": "Typed OpenAPI Client",
  "repository": {
    "type": "git",
    "url": "ardatan/whatwg-node",
    "directory": "packages/oas-client"
  },
  "author": "Arda TANRIKULU <ardatanrikulu@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@whatwg-node/fetch": "0.8.1",
    "@whatwg-node/typed-fetch": "0.0.0",
    "json-schema-to-ts": "2.7.2",
    "openapi-types": "12.1.0",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
